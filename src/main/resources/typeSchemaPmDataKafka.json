{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "type": "object",
   "additionalProperties": false,
   "properties": {
      "pmFilter": {
         "type": "object",
         "additionalProperties": false,
         "properties": {
            "sourceNames": {
               "type": "array",
               "items": [
                  {
                     "type": "string"
                  }
               ]
            },
            "measObjInstIds": {
               "type": "array",
               "items": [
                  {
                     "type": "string"
                  }
               ]
            },
            "measTypes": {
               "type": "array",
               "items": [
                  {
                     "type": "string"
                  }
               ]
            },
            "measuredEntityDns": {
               "type": "array",
               "items": [
                  {
                     "type": "string"
                  }
               ]
            }
         }
      },
      "maxConcurrency": {
         "type": "integer",
         "minimum": 1
      },
      "bufferTimeout": {
         "type": "object",
         "additionalProperties": false,
         "properties": {
            "maxSize": {
               "type": "integer",
               "minimum": 1
            },
            "maxTimeMiliseconds": {
               "type": "integer",
               "minimum": 0,
               "maximum": 160000
            }
         },
         "required": [
            "maxSize",
            "maxTimeMiliseconds"
         ]
      }
   }
}